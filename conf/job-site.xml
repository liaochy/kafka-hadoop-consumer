<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<property>
		<name>fs.defaultFS</name>
		<value>hdfs://nameservice1</value>
	</property>

	<property>
		<name>io.file.buffer.size</name>
		<value>131072</value>
		<description></description>
	</property>
	<property>
		<name>fs.inmemory.size.mb</name>
		<value>200</value>
		<description></description>
	</property>

	<property>
		<name>dfs.block.size</name>
		<value>134217728</value>
		<description>The default block size for new files.</description>
	</property>

	<property>
		<name>io.compression.codecs</name>
		<value>org.apache.hadoop.io.compress.DefaultCodec,org.apache.hadoop.io.compress.GzipCodec,org.apache.hadoop.io.compress.SnappyCodec</value>
	</property>

	<property>
		<name>hdfs.output.compress</name>
		<value>true</value>
	</property>

	<property>
		<name>hdfs.output.compression.codec</name>
		<value>org.apache.hadoop.io.compress.SnappyCodec</value>
	</property>

</configuration>